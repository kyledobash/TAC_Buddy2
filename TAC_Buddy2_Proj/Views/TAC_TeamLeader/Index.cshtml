@model IEnumerable<TAC_Buddy2_Proj.Models.TAC_TeamLeader>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
