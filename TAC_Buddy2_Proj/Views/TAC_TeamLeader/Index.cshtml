            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.TAC_TeamLeader_ID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.TAC_TeamLeader_ID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.TAC_TeamLeader_ID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
